{
  "name": "nx-ultimate-starter",
  "version": "0.0.0",
  "scripts": {
    "cz": "git-cz",
    "prepare": "husky install",
    "start": "nx run-many --projects=api,web --target=serve --parallel=true --maxParallel=2",
    "build": "nx run-many --projects=api,web --target=build --parallel=true --maxParallel=2",
    "test": "nx run-many --all --target=test --parallel=true --maxParallel=3",
    "lint": "nx run-many --all --target=lint --parallel=true --maxParallel=3",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.1.0",
